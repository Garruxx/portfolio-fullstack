---
import LayoutDashboard from '../../layouts/LayoutDashboard.astro';
import DashboardPanel from '@src/react/components/DasboardPanel/DashboardPanel.jsx';
import { StaticPaths } from './config/static-paths.js';
export const getStaticPaths = () => {
	return StaticPaths;
};
const { Content, route } = Astro.props;
---

<LayoutDashboard title="dashboard">
	<section class="flex">
		<DashboardPanel route={route} client:load />
		<Content />
	</section>
</LayoutDashboard>
